{
	"name": "automatic-plugin-manager",
	"version": "1.1.3",
	"main": "main.js",
	"description": "Automatically install, enable, and configure Obsidian community plugins from JSON configuration files. Perfect for syncing plugin setups across devices.",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"plugin-manager",
		"automatic-installation",
		"plugin-sync",
		"vault-configuration",
		"community-plugins"
	],
	"author": "Konstantin Volobuev",
	"license": "MIT",
	"devDependencies": {
		"@types/jszip": "^3.4.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"jszip": "^3.10.1"
	}
}
